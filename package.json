{
  "name": "@logux/stylelint-config",
  "version": "0.4.0",
  "description": "Stylelint shareable config for Logux",
  "keywords": [
    "logux",
    "stylelint",
    "stylelint-config"
  ],
  "scripts": {
    "test": "stylelint test/*.css && yaspeller *.md"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/stylelint-config",
  "dependencies": {
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0"
  },
  "peerDependencies": {
    "stylelint": "^14.0.0"
  },
  "devDependencies": {
    "clean-publish": "^3.4.2",
    "stylelint": "^14.0.0",
    "yaspeller": "^7.2.0"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ],
    "dictionary": [
      "Versioning",
      "Stylelint",
      "Logux"
    ]
  }
}

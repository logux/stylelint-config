{
  "name": "@logux/stylelint-config",
  "version": "0.3.1",
  "description": "Stylelint shareable config for Logux",
  "keywords": [
    "logux",
    "stylelint",
    "stylelint-config"
  ],
  "scripts": {
    "test": "yaspeller *.md"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/stylelint-config",
  "dependencies": {
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0"
  },
  "peerDependencies": {
    "stylelint": "^13.12.0"
  },
  "devDependencies": {
    "clean-publish": "^2.1.1",
    "stylelint": "^13.12.0",
    "yaspeller": "^7.0.0"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ],
    "dictionary": [
      "Versioning",
      "Stylelint",
      "Logux"
    ]
  }
}

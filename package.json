{
  "name": "@logux/stylelint-config",
  "version": "0.5.0",
  "description": "Stylelint shareable config for Logux",
  "keywords": [
    "logux",
    "stylelint",
    "stylelint-config"
  ],
  "scripts": {
    "test": "stylelint test/*.css"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/stylelint-config",
  "dependencies": {
    "stylelint-config-recess-order": "^2.6.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0"
  },
  "peerDependencies": {
    "stylelint": "^14.0.0"
  },
  "devDependencies": {
    "clean-publish": "^3.4.3",
    "stylelint": "^14.0.1"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}

{
  "name": "@logux/stylelint-config",
  "version": "0.16.0",
  "description": "Stylelint shareable config for Logux",
  "keywords": [
    "logux",
    "stylelint",
    "stylelint-config"
  ],
  "scripts": {
    "test": "stylelint test/*.css"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/stylelint-config",
  "dependencies": {
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-gamut": "^1.3.3",
    "stylelint-order": "^6.0.3"
  },
  "peerDependencies": {
    "stylelint": "^15.5.0"
  },
  "devDependencies": {
    "clean-publish": "^4.2.0",
    "stylelint": "^15.10.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"

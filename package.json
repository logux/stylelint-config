{
  "name": "@logux/stylelint-config",
  "version": "0.13.0",
  "description": "Stylelint shareable config for Logux",
  "keywords": [
    "logux",
    "stylelint",
    "stylelint-config"
  ],
  "scripts": {
    "test": "stylelint test/*.css"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/stylelint-config",
  "dependencies": {
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-gamut": "^1.3.1",
    "stylelint-order": "^6.0.2"
  },
  "peerDependencies": {
    "stylelint": "^14.16.0"
  },
  "devDependencies": {
    "clean-publish": "^4.1.1",
    "stylelint": "^15.1.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
